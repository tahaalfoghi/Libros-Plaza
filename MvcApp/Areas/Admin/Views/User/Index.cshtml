@model IQueryable<UserView>

<h1>Users List</h1>
<table class="table table-striped">
    
        <thead class="thead-dark">
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Email</th>
                <th scope="col">Role</th>
            </tr>
        </thead>
        <tbody>
        @foreach(var obj in Model)
        {
            
            <tr>
                <td>@obj.UserName</td>
                <td>@obj.Email</td>
                <td>@obj.Role</td>
            </tr>
        }
        </tbody>
</table>
